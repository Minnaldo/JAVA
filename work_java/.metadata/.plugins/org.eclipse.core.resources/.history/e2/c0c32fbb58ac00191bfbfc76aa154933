package com.ssafy;

public class Book {
	private String isbn;
	private String title;
	private String author;
	private String publisher;
	private int price;
	private String desc;
	
	
	Book() {}
	
	public Book(String isbn, String title, String author, String publisher, int price, String desc)
	{
		this.isbn = isbn;
		this.title = title;
		this.author = author;
		this.publisher = publisher;
		this.price = price;
		this.desc = desc;
	}
	
	
//	public void setYear(int year)
//	{
//		if (year > 1900 || year <= 2100)
//			this.year = year;
//		else
//			System.out.println("입력하신 년도는 범위 밖입니다.");
//	}
//	
//	public int getYear()
//	{
//		return year;
//	}
	
	
	
	
	public String toString()
	{
//		String str = "isbn="+isbn+", title="+title+", author="+author+", publisher="+publisher+", price="+price+", desc="+desc;
		String str = isbn + "   | " + title + "    | " + author + "   | " + publisher + "   | " + price + "   | " + desc;
		return str;
	}
}
