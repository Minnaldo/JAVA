package Scripts;

import java.util.Scanner;

public class HW_Array_03_Chomin_Problem1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		//0~9 개수 출력위해 배열 10개 선언.
		int[] arr = new int[10];
		int[] chk = new int[10];
			
		
		int[] input;
		
//		for(int i = 0; i < 10; i ++)
//			System.out.println(arr[i]);
		
		for(int i = 0; i < input.length; i++)
		{
			input[i] = sc.nextInt();
		}
		
		do
		{
			
			input = sc.nextInt();
			
			if (input == 0)
			{
				chk[0]++;
				break;
			}			
			
			else if (input / 10 == 1)
				chk[1]++;		
			else if (input / 10 == 2)
				chk[2]++;			
			else if (input / 10 == 3)
				chk[3]++;			
			else if (input / 10 == 4)
				chk[4]++;			
			else if (input / 10 == 5)
				chk[5]++;			
			else if (input / 10 == 6)
				chk[6]++;			
			else if (input / 10 == 7)
				chk[7]++;			
			else if (input / 10 == 8)
				chk[8]++;			
			else if (input / 10 == 9)
				chk[9]++;
					
		}while(input == 0);
		
		for (int i = 0; i < arr.length; i++)
		{
			if (arr[i] >= 1)
			{
				System.out.println(arr[i] + " : " + chk[i]);
			}
		}
		
		
	}

}
