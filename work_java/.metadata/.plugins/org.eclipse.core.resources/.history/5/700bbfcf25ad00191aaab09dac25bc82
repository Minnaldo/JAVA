package ssafy_HW_chapr05_190723;

public class BookManager {
	private Book[] books;
	private int max = 10;
	private int index = 0;
	
	public BookManager()
	{
		books = new Book[max];
	}
	
	public void addBook(Book b)
	{
		if (index < max)
		{
			books[index] = b;
			index++;
		}
		else
		{
			Book[] temp = new Book[max * 2];
			max *= 2;
		}
	}
	
}
