package day01;

public class Condition {
	public static void main(String[] args) {

		int a = 3;
		char c = 'a'
		switch (a) {
		case 'a':
			System.out.println(1);
			break;
		case 2:
			System.out.println(2);
			break;
		case 3:
			System.out.println(3);
			break;
		default:
			System.out.println("default");
		}

		System.out.println("종료");
	}
}
