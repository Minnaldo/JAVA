package day01;

public class ArrTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 10;
		int b = 20, c = 40;
		
		int[] Array = new int[3];
		Array[0] = 7;
		Array[0] = 8;
		Array[0] = 9;
		
		int[] Array2 = {10, 20, 40};
		
		for(int i = 0; i < Array2.length; i++)
			System.out.println(Array2[i]);
		
		System.out.println("++++++");
		
		for(int i = 0; i < Array.length; i++)
			System.out.println(Array[i]);
	}

}
