package com.ssafy.Magazine;

public class Magazine {
	private String isbn;
	private String title;
	private String author;
	private String publisher;
	private int price;
	private String desc;
	private int year;
	private int month;
	
	
	public void setYear(int year)
	{
		if (year > 1900 || year <= 2100)
			this.year = year;
		else
			this.year = year;
	}
	
}
