package SWEA_D1;

import java.util.Scanner;

public class SWEA_D1_2058 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int input = sc.nextInt();
		int sum = 0;
		
		for (int i = 1; i < 4; i++)
		{
			sum += (input / 10);
		}
	}
}
