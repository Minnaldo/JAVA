package ssafy_chap04_190722;

/**Person 정보클래스 */
class Person {
	/** 이름 */
	String name;		//이름
	/** 나이 */
	int age;			//나이
	/** 성별 */
	boolean gender;		//성별	//여 : true, 남 : false
	/** 주소 */
	String addr;		//주소
	
	/** Constructor (생성자) */
	Person(){
		System.out.println("생성자");
		name = "박길동";
		age = 21;
		gender = true;
		addr = "첨단";
	}
	
	//정보 메소드
	String info()
	{
		String str = "name="+name+", age="+age+", gender="+gender+", addr="+addr;
		return str;
	}
	
	
}