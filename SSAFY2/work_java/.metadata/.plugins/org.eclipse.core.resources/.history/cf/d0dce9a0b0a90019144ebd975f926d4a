package SWEA_D1;

import java.util.Scanner;

public class SWEA_D1_2027 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int input = sc.nextInt();
		
		for(int i = 0; i < input; i++)
		{
			for(int j = 0; j < i - 1; j++)
			{
				System.out.print("+");
			}
			
			System.out.println();
		}
	}

}
