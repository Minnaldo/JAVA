package ssafy_chap04_190722;

public class Date {
	private int year;
	private int month;
	private int day;
	
	/*데이터에는 private , 기능에는 public 
	 */
	
	public void setYear(int year)
	{
		
	}
	
	public void getYear()
	{
		
	}
	
	public Date() {}
	
	public Date(int year, int month, int day)
	{
		this.year = year;
		this.month = month;
		this.day = day;
	}
	
	public String info()
	{
		return this.year+","+month+","+day;
	}
}
