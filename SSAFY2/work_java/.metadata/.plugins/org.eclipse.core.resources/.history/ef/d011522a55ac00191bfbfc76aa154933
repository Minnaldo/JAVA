package com.ssafy.Magazine;

public class Magazine {
	private String isbn;
	private String title;
	private String author;
	private String publisher;
	private int price;
	private String desc;
	private int year;
	private int month;
	
	
	
//	public String info()
//	{
//		return;
//	}
	
	
	
	
	
	
	public void setYear(int year)
	{
		if (year > 1900 || year <= 2100)
			this.year = year;
		else
			System.out.println("입력하신 년도는 범위 밖입니다.");
	}
	
	public int getYear()
	{
		return year;
	}
	
}
