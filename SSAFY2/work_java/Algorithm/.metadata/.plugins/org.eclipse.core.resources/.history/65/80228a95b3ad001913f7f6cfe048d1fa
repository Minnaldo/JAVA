//중간값찾기 (카운팅 정렬 해보기)
package Algorithm_2WEEK;

import java.util.Scanner;

public class midnum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		
		int[] chk = new int[N + 1];
		int[] input = new int[N];
		int[] sort_input = new int[N];
		int sum = 0, max = -1;
		
		//입력값.
		for(int i = 0; i < N; i++)
		{
			input[i] = sc.nextInt();
			if(max < input[i])
		}
		
		//입력과 동시에 원소 개수 계산.
		for(int i = 0; i < chk.length; i++)
		{
			chk[input[i]]++;
		}
		
		
		
		
		
	}

}
